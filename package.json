{
    "name": "learning-vue",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "server:start": "python -c 'import api; api.app.run(port=8082)'",
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build",
        "lint": "vue-cli-service lint",
        "test:server": "python -m pytest",
        "test:unit": "vue-cli-service test:unit",
        "test:e2e": "vue-cli-service test:e2e",
        "test:e2e-assuming-server-is-running": "export SERVER_IS_RUNNING=yes ; vue-cli-service test:e2e"
    },
    "dependencies": {
        "axios": "^0.18.0",
        "vue": "^2.5.17",
        "vuex": "^3.0.1"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "^3.0.4",
        "@vue/cli-plugin-e2e-nightwatch": "^3.0.4",
        "@vue/cli-plugin-eslint": "^3.0.4",
        "@vue/cli-plugin-unit-mocha": "^3.0.4",
        "@vue/cli-service": "^3.0.4",
        "@vue/test-utils": "^1.0.0-beta.20",
        "chai": "^4.1.2",
        "sinon": "^6.3.5",
        "vue-template-compiler": "^2.5.17",
        "yop-promises": "^1.3.0"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/essential",
            "eslint:recommended"
        ],
        "rules": {},
        "parserOptions": {
            "parser": "babel-eslint"
        }
    },
    "postcss": {
        "plugins": {
            "autoprefixer": {}
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 8"
    ]
}
